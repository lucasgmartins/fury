{
  "app": {
    "name" : "fury",
    "port": 9000,
    "uri": "http://localhost:9000",
    "context" : "/psp"
  },
  "mongo": {
    "host"      : "docker",
    "database"  : "psp-local",
    "options": {
      "autoReconnect"    : true,
      "poolSize"          : 10,
      "reconnectTries"    : 5,
      "reconnectInterval" : 2000
    }
  },
  "jwt": {
    "secret" : "secret_token"
  }
}